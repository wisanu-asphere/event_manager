@*@model IEnumerable<project_eventmanager.Models.tag_event_log>*@

<div class="card">
    <div class="card-header">
        <h3 class="card-title">EVENT LIST</h3>
        <div class="card-tools">
            <div class="card-tools">
                <button type="button" class="btn btn-tool" data-card-widget="collapse">
                    <i class="fas fa-minus"></i>
                </button>
                <button type="button" class="btn btn-tool" data-card-widget="remove">
                    <i class="fas fa-times"></i>
                </button>
            </div>
        </div>
        <!-- /.card-tools -->
    </div>
    <!-- /.card-header -->
    <div class="card-body">
        <table id="table_ListTag" class="table table-bordered table-striped hover">
            <thead>
                <tr>
                    <th>
                        Tag
                    </th>
                    <th>
                        State Start
                    </th>
                    <th>
                        Event Start
                    </th>
                    <th>
                        State End
                    </th>
                    <th>
                        Event End
                    </th>
                    <th>
                        Lastmodify
                    </th>
                    <th>
                        Value
                    </th>

                </tr>
            </thead>
            <tbody>

                <tr>
                    <td>
                        tag_name
                    </td>
                    <td>
                        state_start
                    </td>
                    <td>
                        event_start
                    </td>
                    <td>
                        state_end
                    </td>
                    <td>
                        event_end
                    </td>
                    <td>
                        lastmodify
                    </td>
                    <td>
                        last_value
                    </td>
                </tr>
                
            </tbody>
        </table>
        @* <table id="assets-data-table" class="table table-striped table-bordered" style="width:100%;"></table>*@
    </div>
</div>